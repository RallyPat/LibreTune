{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/"],
  "entryPointStrategy": "expand",
  "out": "../../docs/api",
  "name": "LibreTune Frontend API",
  "readme": "README.md",
  "plugin": ["typedoc-plugin-missing-exports"],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "exclude": [
    "**/node_modules/**",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/vite-env.d.ts"
  ],
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Components",
    "Hooks",
    "Types",
    "Utilities",
    "*"
  ],
  "navigationLinks": {
    "GitHub": "https://github.com/RallyPat/LibreTune",
    "Rust API Docs": "../doc/libretune_core/index.html",
    "User Manual": "../book/index.html"
  },
  "includeVersion": true,
  "gitRevision": "main"
}
